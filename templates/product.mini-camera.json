        "heading_M36whR": {
          "type": "heading",
          "settings": {
            "text": "Toute la Puissance d'une Caméra d'Usine,  Dans la Taille d'une Gomme.",
            "heading_tag": "h3",
            "text_color": "",
            "gradient": ""
          }
        },
        "image_AVV7zj": {
          "type": "image",
